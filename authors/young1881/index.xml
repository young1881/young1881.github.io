    <item>
      <title>书评：《货币的非国家化》</title>
      <link>/posts/%E4%B9%A6%E8%AF%84%E8%B4%A7%E5%B8%81%E7%9A%84%E9%9D%9E%E5%9B%BD%E5%AE%B6%E5%8C%96/</link>
